{
  "name": "Rhino Steps Recorder and Application Lifecycle Management Integrator",
  "manifest_version": 2,
  "version": "0.0.0.5",
  "description": "Allows the creation of test cases and integrate them into an Application Life Cycle Management of your choice",
  "key": "",
  "permissions": [
    "declarativeContent",
    "storage",
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "options_page": "user-interface/options.html",
  "background": {
    "scripts": [
      "js/utilities.http.js",
      "js/utilities.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*/*"
      ],
      "js": [
        "js/utilities.http.js",
        "js/utilities.js",
        "js/middleware.js",
        "js/integration.html.js",
        "js/integration.js",
        "js/integration.azure.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "user-interface/popup.html",
    "default_icon": {
      "16": "images/i16.png",
      "32": "images/i32.png",
      "48": "images/i48.png",
      "128": "images/i128.png"
    }
  },
  "icons": {
    "16": "images/i16.png",
    "32": "images/i32.png",
    "48": "images/i48.png",
    "128": "images/i128.png"
  }
}