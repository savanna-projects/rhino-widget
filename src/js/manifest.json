{
  "name": "Rhino Steps Recorder and Application Lifecycle Management Integrator",
  "manifest_version": 2,
  "version": "0.0.0.5",
  "description": "Allows the creation of test cases and integrate them into an Application Life Cycle Management of your choice",
  "key": "",
  "permissions": [
    "declarativeContent",
    "storage",
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        [
          "*://*/*"
        ]
      ],
      "js": [
        "js/integration_utilities.js",
        "js/integration_xray_cloud.js",
        "js/integration_xray_onprem.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "user-interface/popup.html",
    "default_icon": {
      "16": "images/i16.png",
      "32": "images/i32.png",
      "48": "images/i48.png",
      "128": "images/i128.png"
    }
  },
  "icons": {
    "16": "images/i16.png",
    "32": "images/i32.png",
    "48": "images/i48.png",
    "128": "images/i128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://rhino-widget.azurewebsites.net/*"
    ],
    "accepts_tls_channel_id": false
  }
}